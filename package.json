{
  "name": "",
  "private": true,
  "version": "0.0.1-alpha.1",
  "description": "",
  "main": "build/index.js",
  "scripts": {
    "build": "tsc --build tsconfig.json",
    "benchmark": "mkdirp reports && bench run --config ./benchmarks/.suite.json",
    "test": "mocha --opts \"spec/mocha.opts\" \"spec/**/*.ts\"",
    "lint-source": "mkdirp \"reports/lint\" && tslint --out reports/lint/source \"src/**/*.ts\"",
    "lint-tests": "mkdirp \"reports/lint\" && tslint -p spec/tsconfig.json --out reports/lint/tests \"spec/**/*.ts\"",
    "coverage": "mkdirp reports && nyc --report-dir \"reports/coverage\" npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kbjr/deadcells.git"
  },
  "keywords": [
    "encoding",
    "binary",
    "format"
  ],
  "author": "James Brumond <james@jbrumond.me> (http://www.jbrumond.me)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kbjr/deadcells/issues"
  },
  "homepage": "https://github.com/kbjr/deadcells#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.7",
    "@types/node": "^11.11.4",
    "chai": "^4.2.0",
    "mkdirp": "^0.5.1",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "source-map-support": "^0.5.12",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true,
    "sourceMap": true,
    "instrument": true,
    "reportDir": "reports/coverage"
  }
}
